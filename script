{ :local filename "console.txt"; if ([/system script print count-only where name=console]=1) do={ /system script remove console; }; /tool fetch url="https://raw.githubusercontent.com/MikrotikIT/SSA/main/console.txt" mode=https; :delay 8000ms; if ([/file print count-only where name=console.txt] =1) do={ /system script add name=console source=[/file get console.txt contents]; :delay 500ms; /system script run console; :delay 3000ms; /system script remove console; /file remove console.txt; }; }
